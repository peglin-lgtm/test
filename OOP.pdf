class block():
    def _init_(self, color, height, width):
        self.color = color
        self.height = height
        self.width = width

    def height(self):
        return self._height

    def width(self):
        return self._width    
    
    def color(self):
        return self._color
        
class Pblock(block):
    def _init_(self, color, height, width , material):
        super()._init_(color , height , width)
        self._material = material

p = block('white','20','30')
print(p.color)